<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ScrumPokerCards.Pages.CardViewPage"
             Title="{Binding Card.Title}">
    <ContentPage.Content>
        <Frame Padding="0" Margin="10" VerticalOptions="Center" HorizontalOptions="Center">
            <Image Source="{Binding Card.Image}" />
        </Frame>
    </ContentPage.Content>
</ContentPage>